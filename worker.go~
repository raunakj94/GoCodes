package main
import "fmt"
func main() {
    start := time.Now()

    in := make(chan int)    // Channel on which work orders are received.
    out := make(chan []int) // Channel on which results are returned.
    go producer(in)
    go worker(in, out) // Launch one worker.
    consumer(out, 100)

    fmt.Println(time.Since(start))
}
